---
import { Education } from "../components/Education";
import config from "../../meta.config";
const { education } = config;
---

<div class="mx-4">
  <h1 class="mb-4 text-2xl text-white-700 font-bold border-l-4 border-yellow-600 bg-yellow-600/10 p-1 pl-4">教育背景</h1>
  <ul>
    <li class="mb-6 flex justify-between">
      {
        education.map((name) => {
          return <Education name={name} client:load />;
        })
      }
    </li>
  </ul>
</div>
